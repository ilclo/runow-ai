{
  "coach_version": "1.0.0",
  "profile": "intermedio",
  "verbosity": "standard",
  "session_defaults": {
    "distance_km": 5.0,
    "goal_mode": "distance"            // distance | time
  },
  "toggles": {
    "hr": true,
    "pace": true,
    "cadence": true,
    "split": true,
    "last400m": true
  },
  "metronome": "OUT_OF_RANGE",         // OFF | OUT_OF_RANGE | ALWAYS
  "caps": {
    "messages_per_10min": 7,
    "metronome_per_10min": 2
